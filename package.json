    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-remark": "^6.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "inquirer": "^9.2.8",
    "lodash": "^4.17.11",
    "nodemailer": "^8.0.1",
    "path": "^0.12.7",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^6.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop-local": "./node_modules/gatsby/dist/bin/gatsby.js develop",
    "test": "npm run lint:css && npm run lint:js && npm run check-pretty",
    "setup": "node ./bin/setup.js",
    "dev": "gatsby develop",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier -l {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\"",
    "serve-local": "./node_modules/gatsby/dist/bin/gatsby.js serve",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "gatsby-plugin-postcss": "^6.11.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
